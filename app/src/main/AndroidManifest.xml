<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zemu.fresco">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FrescoSpimgActivity" />
        <activity android:name=".FrescoCircleAndCornerActivity" />
        <activity android:name=".FrescoJpegActivity" />
        <activity android:name=".FrescoGifAcitivity" />
        <activity android:name=".FrescoMultiActivity" />
        <activity android:name=".FrescoAutoSizeActivity" />
        <activity android:name=".FrescoListenerActivity" />
        <activity android:name=".FrescoResizeActivity" />
        <activity android:name=".FrescoUpdateActivity" />
        <activity android:name=".FrescoCropActivity"></activity>
    </application>

</manifest>